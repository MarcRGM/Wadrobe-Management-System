<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>


<VBox stylesheets="@CSS/Items.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.wardrobemanagementsystem.ItemsController">
   <children>
      <Button fx:id="btnItem" maxHeight="220.0" maxWidth="210.0" minHeight="220.0" minWidth="210.0" mnemonicParsing="false" onAction="#btnItemClicked" prefHeight="220.0" prefWidth="210.0">
         <graphic>
            <ImageView fx:id="imgItem" fitHeight="220.0" fitWidth="210.0" pickOnBounds="true" preserveRatio="true" />
         </graphic>
      </Button>
   </children>
</VBox>
